{
  "name": "professorp-website",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "0.10",
    "npm": "1.3"
  },
  "dependencies": {
    "browserify": "^4.1.10",
    "browserify-shim": "^3.5.0",
    "docpad": "^6.68.0",
    "docpad-plugin-browserifydocs": "~2.2.0",
    "docpad-plugin-cleanurls": "~2.7.0",
    "docpad-plugin-coffeescript": "~2.4.0",
    "docpad-plugin-cson": "~2.1.0",
    "docpad-plugin-eco": "~2.0.3",
    "docpad-plugin-htmlmin": "~2.4.0",
    "docpad-plugin-marked": "~2.2.1",
    "docpad-plugin-partials": "~2.9.1",
    "docpad-plugin-sass": "~2.7.2",
    "ftpsync": "^0.1.6",
    "moment": "^2.7.0"
  },
  "devDependencies": {
    "docpad-plugin-livereload": "~2.6.0"
  },
  "main": "node_modules/docpad/bin/docpad-server",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "underscore": "global:_",
    "jquery": "global:$"
  },
  "scripts": {
    "start": "docpad run",
    "deploy": "docpad generate --out build --env production && ftpsync --verbose --local ./build --host professorp.co.uk --connections 2"
  }
}
