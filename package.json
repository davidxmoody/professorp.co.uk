{
  "name": "memory-game",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "browserify": "^3.46.0",
    "browserify-shim": "^3.4.1",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.6.0",
    "gulp": "^3.6.2",
    "gulp-util": "^2.2.14",
    "http-server": "^0.6.1",
    "uglifyify": "^2.1.1",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^0.8.3"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000",
    "gulp": "gulp --require coffee-script/register"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim",
      "uglifyify"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "underscore": "global:_"
  }
}
